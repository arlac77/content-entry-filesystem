{
  "name": "content-entry-filesystem",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./src/content-entry-filesystem.mjs"
  },
  "description": "Content entries within the file system",
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 4m tests/*-ava.mjs tests/*-ava-node.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 4m tests/*-ava.mjs tests/*-ava-node.mjs && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme --section=API ./src/**/*.mjs",
    "lint": "npm run lint:docs && npm run lint:tsc",
    "lint:docs": "documentation lint ./src/**/*.mjs",
    "lint:tsc": "tsc --allowJs --checkJs --noEmit -t esnext -m esnext --module nodenext --moduleResolution nodenext ./src**/*.mjs"
  },
  "dependencies": {
    "content-entry": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "ava": "^6.1.1",
    "c8": "^9.1.0",
    "documentation": "^14.0.3",
    "semantic-release": "^23.0.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.18.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/content-entry-filesystem.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/content-entry-filesystem/issues"
  },
  "homepage": "https://github.com/arlac77/content-entry-filesystem#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-esm-only",
      "arlac77/template-typescript"
    ]
  }
}
