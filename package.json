{
  "name": "content-entry-filesystem",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "exports": {
    ".": "./src/content-entry-filesystem.mjs"
  },
  "description": "Content entries within the file system",
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m tests/*.mjs && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme --section=API ./src/**/*.mjs",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/**/*.mjs"
  },
  "dependencies": {
    "content-entry": "^2.6.5"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "c8": "^7.8.0",
    "documentation": "^13.2.5",
    "semantic-release": "^17.4.6"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/content-entry-filesystem.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/content-entry-filesystem/issues"
  },
  "homepage": "https://github.com/arlac77/content-entry-filesystem#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-esm-only"
    ]
  }
}
